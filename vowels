 public static void main(String args[])
  {
	  char  a,e,i,o,u;
		  Scanner sc=new Scanner(System.in);
		  System.out.println("Enter your choice:");
		  char ch=sc.next().charAt(0);
	  switch(ch)
	  {
		  case 'a':
	      System.out.println("its a vowel");
		  break;
		  case 'e':
		  System.out.println("its a vowel");
		  break;
		  case 'i':
		  System.out.println("its a vowel");
		  break;
		  case 'o':
		  System.out.println("its a vowel");
		  break;
		  case 'u':
		  System.out.println("its a vowel");
		  break;
		  default:
		  System.out.println("its a consonent");
		  break;
	  }
  }
 }
